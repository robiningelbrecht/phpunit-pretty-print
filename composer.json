{
  "name": "robiningelbrecht/phpunit-pretty-print",
  "description": "Prettify PHPUnit output",
  "keywords": [
    "Testing",
    "PHP",
    "phpunit"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Robin Ingelbrecht",
      "email": "ingelbrecht_robin@hotmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "nunomaduro/collision": "^7.8|^8.0",
    "phpunit/phpunit": "^10.3|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "RobinIngelbrecht\\PHPUnitPrettyPrint\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.9",
    "phpstan/phpstan": "^1.10",
    "spatie/phpunit-snapshot-assertions": "^5.0"
  },
  "config": {
    "sort-packages": true
  }
}
